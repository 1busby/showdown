{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && mkdir -p dist/server/public/brackets-client && cp -r ../client/dist/brackets-client dist/server/public",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/server/src/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "pm2 start dist/server/src/main.js",
    "preprod": "rimraf dist && npm run build",
    "prod": "node dist/server/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db": "npm run docker-build",
    "docker-build": "cd .. && docker build -t mbusbydocker/brackets -f Dockerfile .",
    "dr": "npm run docker-run",
    "docker-run": "docker run -p 3000:3000 mbusbydocker/brackets",
    "hb": "npm run heroku-build",
    "heroku-build": "cd .. && heroku container:push web -a busby-brackets --arg DATABASE_URL=\"mongodb+srv://alex:fdbCymPdDMntDfUT@cluster0-cez0a.mongodb.net/brackets?retryWrites=true&w=majority\"",
    "hr": "npm run heroku-release",
    "heroku-release": "heroku container:release web -a busby-brackets",
    "ho": "npm run heroku-open",
    "heroku-open": "heroku open -a busby-brackets",
    "hl": "npm run heroku-logs",
    "heroku-logs": "heroku logs --tail -a busby-brackets"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/config": "^0.4.1",
    "@nestjs/core": "^7.0.9",
    "@nestjs/graphql": "^7.3.7",
    "@nestjs/mongoose": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.9",
    "apollo-server-express": "^2.9.15",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "mongoose": "^5.8.4",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pm2": "^4.4.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.4",
    "shortid": "^2.2.15",
    "type-graphql": "^0.17.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/testing": "^6.10.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^23.3.13",
    "@types/mongoose": "^5.5.38",
    "@types/node": "^10.17.13",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "concurrently": "^4.1.2",
    "jest": "^23.6.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.8.3",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
